  },
  exampleText: { flex: 1, fontSize: 14 },
  generateButton: {
    flexDirection: 'row',
    alignItems: 'center',
    justifyContent: 'center',
    padding: 16,
    borderRadius: 12,
    gap: 8,
  },
  generateButtonText: { color: '#FFFFFF', fontSize: 18, fontWeight: '600' },

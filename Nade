// Create directory structure
const dirs = ['src', 'src/context', 'src/screens'];
dirs.forEach(dir => {
  if (!fs.existsSync(dir)) {
    fs.mkdirSync(dir, { recursive: true });
    console.log('âœ… Created directory:', dir);
  }
});

